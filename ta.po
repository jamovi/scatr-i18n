#: scat/options.description.R.usage
msgid "set.seed(1337)\n"
"\n"
"X <- rnorm(100)\n"
"Y <- 0.5*X + rnorm(100)\n"
"dat <- data.frame(X = X, Y = Y)\n"
"\n"
"scat(dat, x = 'X', y = 'Y', line = 'linear', se = TRUE, marg = 'dens')"
msgstr ""
"set.seed (1337)\n"
"\n"
" ஃச் <- rnorm (100)\n"
" ஒய் <- 0.5*x + rnorm (100)\n"
" dat <- data.frame (x = ஃச், ஒய் = y)\n"
"\n"
" ச்காட் (dat, ஃச் = 'x', ஒய் = 'y', வரி = 'நேரியல்', Se = true, marg = 'dens')"

#: pareto/options.description.R.usage
msgid "set.seed(1337)\n"
"\n"
"X <- sample(c('A','B','C','D','E','F'), 100, replace=TRUE)\n"
"dat <- data.frame(X = X)\n"
"\n"
"pareto(dat, x = 'X')"
msgstr ""
"set.seed (1337)\n"
"\n"
" ஃச் <- மாதிரி (சி (சி ('ஏ', 'பி', 'சி', 'டி', 'இ', 'எஃப்'), 100, மாற்றவும் = "
"உண்மை)\n"
" dat <- data.frame (x = x)\n"
"\n"
" பரேட்டோ (dat, ஃச் = 'x')"

