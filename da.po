#: scat/options.description.R.usage
msgid ""
"set.seed(1337)\n"
"\n"
"X <- rnorm(100)\n"
"Y <- 0.5*X + rnorm(100)\n"
"dat <- data.frame(X = X, Y = Y)\n"
"\n"
"scat(dat, x = 'X', y = 'Y', line = 'linear', se = TRUE, marg = 'dens')"
msgstr ""
"set.seed(1337)\n"
"\n"
"X <- rnorm(100)\n"
"Y <- 0.5*X + rnorm(100)\n"
"dat <- data.frame(X = X, Y = Y)\n"
"\n"
"scat(dat, x = 'X', y = 'Y', line = 'linear', se = TRUE, marg = 'dens')"

#: pareto/options.description.R.usage
msgid ""
"set.seed(1337)\n"
"\n"
"X <- sample(c('A','B','C','D','E','F'), 100, replace=TRUE)\n"
"dat <- data.frame(X = X)\n"
"\n"
"pareto(dat, x = 'X')"
msgstr ""
"set.seed(1337)\n"
"\n"
"X <- sample(c('A','B','C','D','E','F'), 100, replace=TRUE)\n"
"dat <- data.frame(X = X)\n"
"\n"
"pareto(dat, x = 'X')"

